{
  "name": "fetch-plus",
  "description": "A convenient Fetch API library with first-class middleware support",
  "version": "3.8.2",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/RickWong/fetch-plus.git"
  },
  "homepage": "https://github.com/RickWong/fetch-plus",
  "keywords": [
